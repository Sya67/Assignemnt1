#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <ctype.h>

int main{

void inputFile(int argc, char *argv[]) {
     FILE *inputFile;
     FILE* output;
     FILE* key;
     char fn[255]; //ASCII character
     char subs[26]; // Number of Alphabets

     if(argc < 2) //argc refering to the number of arguments passed
     {
      printf("Input file:  \n"); //Asks user for the input file
     }
       inputFile = fopen(argv[1], "r");
       sprintf("fn, %s", argv[1]);
       output = fopen(fn, "w");
       key = fopen("key", "w");

       //Substitution

       for (int i= 0; i<26; i++) {
           fprintf("key, %c", i+65):
       }
        for(int i = 0; 1<26; i++){
          print(key, %c, subs[i]=65);
        }

       int i ;
       while(feof(inputFile)){
        char c;
        if(( c > 64) && (c < 91)) {
            c = subs[c -65]+65;
        printf("Decrypted text is : \n");
        }
       }
       retuen 0;
     }
     
     }
